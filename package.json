{
  "name": "interrep-reputation-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn start:telegramBot & next dev",
    "build": "yarn lint && NODE_OPTIONS='--max-old-space-size=1536' next build --debug",
    "analyze": "ANALYZE=true yarn build",
    "start": "yarn start:telegramBot & next start",
    "script": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register",
    "seed:zeroHashes": "npm run script -- scripts/seedZeroHashes.ts",
    "seed:twitterUsers": "npm run script -- scripts/seedTwitterUsers.ts",
    "start:telegramBot": "npm run script -- scripts/startTelegramBot.ts",
    "test": "jest",
    "test:w": "jest --watch",
    "test:prod": "yarn lint && yarn test",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "cacheDirectories": [
    "node_modules/",
    ".next/cache/"
  ],
  "dependencies": {
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/raleway": "^4.5.0",
    "@interrep/data-models": "^0.1.4",
    "@interrep/reputation-criteria": "^0.12.0",
    "@interrep/semethid": "^0.9.1",
    "@interrep/telegram-bot": "^0.2.2",
    "@sentry/nextjs": "^6.12.0",
    "bnc-onboard": "^1.34.1",
    "botometer": "^1.0.12",
    "circomlib": "^0.5.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.4.5",
    "framer-motion": "^4",
    "mongoose": "^5.12.1",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "react": "17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "twitter-v2": "^1.0.8",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@next/bundle-analyzer": "^10.2.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/cors": "^2.8.12",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^6.0.0",
    "incrementalquintree": "^1.0.7",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "mongodb-memory-server": "^7.3.6",
    "msw": "^0.29.0",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "ts-jest": "^26.5.4",
    "typescript": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}